{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app:handler"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "build": {
    "installCommand": "pip install -r requirements.txt"
  }
}
